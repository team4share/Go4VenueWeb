<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.go4venue.web.persistence.beans.VenueImage" table="venue_images">
		<id name="key" type="java.lang.Long" column="id">
			<generator class="native"/>
		</id>
		
		<property name="venueId" column="venue_id" type="java.lang.Long" />	
		<property name="imagePath" column="image_path" type="java.lang.String" />	
		<property name="remarks" column="remarks" type="java.lang.String" />		
	</class>
</hibernate-mapping>