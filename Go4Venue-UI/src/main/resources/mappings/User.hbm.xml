<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.go4venue.web.persistence.beans.User" table="user" entity-name="user">
		<id name="key" type="java.lang.Integer" column="id">
			<generator class="native"/>
		</id>
		<property name="firstName" column="first_name" type="java.lang.String" />		
		<property name="lastName" column="last_name" type="java.lang.String" />
		<property name="email" column="email" type="java.lang.String" />
		<property name="password" column="password" type="java.lang.String" />
		<property name="contactNumber" column="contact_number" type="java.lang.String" />
		<property name="userType" column="user_type" type="java.lang.String" />
	</class>
</hibernate-mapping>